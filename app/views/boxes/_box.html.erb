<tr>
  <td><%= box_counter + 1 %></td>
  <td><%= box.material.name %></td>
  <td><%= box.material_weight %> lbs</td>
  <td><%= box.location.name %></td>
  <td id="add-boxes"><%= link_to "Delete", job_box_path(box.job, box), method: :delete, data: { confirm: 'Are you certain you want to delete this?' }, class: "btn btn-danger" %></td>
</tr>
