<div class="col-md-12 text-right">
  <div class="row">
    <ul class="">
      <li style="list-style-type: none;"><h4>Create New Job</h4></li>
      <li id="new-job" style="list-style-type: none;"><%= link_to "New Job", new_job_path, class: "btn btn-primary" %></li>
    </ul>
  </div>
</div>
<div class="col-md-12 text right">
  <div class="row">
    <h2>Jobs</h2>
    <table id="myTable" class="tablesorter table tablesorter-default">
      <thead>
        <tr>
          <th> Job Number </th>
          <th> Customer Name </th>
          <th data-sorter="false"> Add Boxes </th>
          <th data-sorter="false"> Add Machines </th>
          <th> Kickoff </th>
          <th> Job Status </th>
          <th> On Hold </th>
          <th> Complete Job </th>
          <th> Delete Job </th>
        </tr>
      </thead>
      <tbody>
        <%= render @jobs %>
      </tbody>
    </table>
  </div>
</div>
