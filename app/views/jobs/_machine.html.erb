<div class="col-xs-12">
  <div class="">
    <p><%= machine.name %></p>
    <div class="col-xs-6">
      <div class="">
        <h3>Input Boxes</h3>
        <table class="table">
          <thead>
            <tr>
              <th> Box Number </th>
              <th> Box Weight </th>
              <th> Location </th>
            </tr>
          </thead>
          <tbody>
            <%= render partial: 'box', collection: machine.input_boxes %>
          </tbody>
        </table>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="">
        <h3>Output Boxes</h3>
        <table class="table">
          <thead>
            <tr>
              <th> Box Number </th>
              <th> Box Weight </th>
              <th> Location </th>
            </tr>
          </thead>
          <tbody>
            <%= render partial: 'box', collection: machine.output_boxes %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
